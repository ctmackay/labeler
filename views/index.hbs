<div class="container-fluid">
  <div class="row">
    <nav class="navbar navbar-light col-md-12" id="top_bar">
      <a class="navbar-brand" href="#">
        <img src="images/logo.png" class="d-inline-block align-top logo animated flipInX" alt="Image Labeler">
      </a>
    </nav>
  </div>

  <div class="row">
    {{ filenames }}
  </div>
  <div class="row">
    <div class="col-6">
      <canvas id="picture_box" width="800" height="450"></canvas>
      <canvas id="rect_box" width="800" height="450"></canvas>
    </div>
    <div class="col-6">
      <div class="" id="gallery">

          {{#each files}}
            <span>
              <img src="images/grapes/{{ this }}" alt="">
            </span>
          {{/each}}


      </div>
    </div>

  </div>

  <div class="row" id="button_group">

    <div class="col-1"></div>
      <div class="col-1">
        <button type="button" class="btn btn-info" id="btnPrevious">Previous</button>
      </div>
      <div class="col-1">
        <button type="button" class="btn btn-info" id="btnNext">Next</button>
      </div>
      <div class="col-9">
      </div>
    </div>

</div>
